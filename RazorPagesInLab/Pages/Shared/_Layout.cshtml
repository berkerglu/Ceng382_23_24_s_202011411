<!DOCTYPE html>
<html lang="en" data-bs-theme="auto">
    <head>
        
        <title>Login</title>
        
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/MyCss.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/CommonCss.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/RazorPagesInLab.styles.css" asp-append-version="true" />

            <!-- Favicons -->
        <link rel="apple-touch-icon" href="~/images/user.png" sizes="180x180">
        <link rel="icon" href="~/images/user32x32.png" sizes="32x32" type="image/png">
        <link rel="icon" href="~/images/user16x16.png" sizes="16x16" type="image/png">
        <link rel="manifest" href="~/json/manifest.json">
        <link rel="mask-icon" href="~/images/user.png" color="#712cf9">
        <link rel="icon" href="~/images/user.png">
        <meta name="theme-color" content="#712cf9">
    </head>
    <body>
        
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
        
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
        <script src="~/js/color-model.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>